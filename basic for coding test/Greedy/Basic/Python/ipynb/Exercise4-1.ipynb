   "source": [
    "N, K = map(int, input().split())\n",
    "count = 0\n",
    "\n",
    "while N >= K:\n",
    "    if N%K != 0:\n",
    "        count += N%K\n",
    "        N -= N%K\n",
    "    else:\n",
    "        N //= K\n",
    "        count += 1\n",
    "        \n",
    "if N > 1:\n",
    "    count += (N-1)\n",
    "        \n",
    "print(count)"
   ]
  }
