   "source": [
    "N = int(input())\n",
    "data = list(map(int, input().split()))\n",
    "data.sort()\n",
    "\n",
    "target = 1 # 만들 수 없는 금액 탐색\n",
    "\n",
    "for i in data:\n",
    "    if target < i: # i 까지 값을 더했는데도 타겟 값이 더 작다면, 만들 수 없는 수\n",
    "        break\n",
    "    target += i\n",
    "    \n",
    "print(target)"
   ]
  }
